{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/convert/route.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "headers": [
  {
    "source": "/api/(.*)",
    "headers": [
      {
        "key": "Cache-Control",
        "value": "no-store, no-cache"
      }
    ]
  }]
}